{
  "presets": [
    ["@babel/env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "safari >= 7"]
      },
      "useBuiltIns": "usage",
      "debug": true
    }]
  ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-syntax-object-rest-spread"
    ]
}